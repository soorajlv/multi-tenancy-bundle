{
    "name": "mapeveri/multi-tenancy-bundle",
    "description": "Multi-tenancy, is a package for symfony and doctrine to manage tenants in a simple way.",
    "version": "1.0.1",
    "type": "symfony-bundle",
    "keywords": ["symfony", "bundle", "doctrine", "multi tenancy", "php"],
    "homepage": "https://github.com/mapeveri/multi-tenancy-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "MartÃ­n Peveri",
            "email": "martinpeveri@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "doctrine/annotations": "^1.0|^2.0",
        "doctrine/doctrine-bundle": "^2.13",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.3",
        "symfony/console": "5.2.*|6.*|7.*",
        "symfony/property-access": "5.2.*|6.*|7.*",
        "symfony/property-info": "5.2.*|6.*|7.*",
        "symfony/serializer": "5.2.*|6.*|7.*",
        "symfony/uid": "5.2.*|6.*|7.*",
        "symfony/yaml": "5.2.*|6.*|7.*",
        "symfony/expression-language": "5.2.*|6.*|7.*",
        "symfony/event-dispatcher": "5.2.*|6.*|7.*"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.2"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "MultiTenancyBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MultiTenancyBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
